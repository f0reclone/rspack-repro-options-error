#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/andreas.neufeld/Code/rspack-repro-options-error/node_modules/.pnpm/@rspack+cli@0.4.3_@rspack+core@0.4.3_webpack-cli@5.1.4/node_modules/@rspack/cli/bin/node_modules:/Users/andreas.neufeld/Code/rspack-repro-options-error/node_modules/.pnpm/@rspack+cli@0.4.3_@rspack+core@0.4.3_webpack-cli@5.1.4/node_modules/@rspack/cli/node_modules:/Users/andreas.neufeld/Code/rspack-repro-options-error/node_modules/.pnpm/@rspack+cli@0.4.3_@rspack+core@0.4.3_webpack-cli@5.1.4/node_modules/@rspack/node_modules:/Users/andreas.neufeld/Code/rspack-repro-options-error/node_modules/.pnpm/@rspack+cli@0.4.3_@rspack+core@0.4.3_webpack-cli@5.1.4/node_modules:/Users/andreas.neufeld/Code/rspack-repro-options-error/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/andreas.neufeld/Code/rspack-repro-options-error/node_modules/.pnpm/@rspack+cli@0.4.3_@rspack+core@0.4.3_webpack-cli@5.1.4/node_modules/@rspack/cli/bin/node_modules:/Users/andreas.neufeld/Code/rspack-repro-options-error/node_modules/.pnpm/@rspack+cli@0.4.3_@rspack+core@0.4.3_webpack-cli@5.1.4/node_modules/@rspack/cli/node_modules:/Users/andreas.neufeld/Code/rspack-repro-options-error/node_modules/.pnpm/@rspack+cli@0.4.3_@rspack+core@0.4.3_webpack-cli@5.1.4/node_modules/@rspack/node_modules:/Users/andreas.neufeld/Code/rspack-repro-options-error/node_modules/.pnpm/@rspack+cli@0.4.3_@rspack+core@0.4.3_webpack-cli@5.1.4/node_modules:/Users/andreas.neufeld/Code/rspack-repro-options-error/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@rspack/cli/bin/rspack" "$@"
else
  exec node  "$basedir/../@rspack/cli/bin/rspack" "$@"
fi
